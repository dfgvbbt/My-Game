const all = [
  'https://media.discordapp.net/attachments/781481778795118612/1025291814774788176/unknown.png',
  'https://media.discordapp.net/attachments/956047653639163935/963619056806682684/unknown.png',
  'https://waifugame.com/originals/G-4837047@2X.jpg',
  'https://cdn.discordapp.com/banners/984935633900429313/a_3348c350741a11a4d89ab16efabf10d4.gif'];
  
  const randomnumber = Math.floor(Math.random() * all.length);
  src = all[randomnumber],
  img = document.createElement('img');

  img.src = src;
  document.body.appendChild(img);
